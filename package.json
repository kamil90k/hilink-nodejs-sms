    "@types/node": "13.1.7",
    "@types/xml2js": "0.4.5",
    "jest": "24.9.0",
    "nock": "11.7.2",
    "ts-jest": "24.3.0",
    "ts-node": "8.6.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "4.1.6"
